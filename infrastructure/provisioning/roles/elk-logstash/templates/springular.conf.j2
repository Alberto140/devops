input
{
    beats
    {
        ssl => false
        port => 5000
        codec => "json"
    }
}
filter {
   if [tags][json] {
      json {
        source => "message"
      }
    }
 }
output {
   # stdout { codec => rubydebug }
   # file {
   #    path => "/home/vagrant/logstash.log"
   #    codec => rubydebug
   # }
    elasticsearch {
      hosts => ["{{elasticsearch_server}}"]
      manage_template => false
      index => "logstash-%{+YYYY.MM.dd}"
   }
}